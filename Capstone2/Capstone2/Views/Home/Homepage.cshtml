    <script>
        document.addEventListener("DOMContentLoaded", () => {
          const buttons = document.querySelectorAll(".category");
          const grids = document.querySelectorAll(".menu-grid");

          function activateCategory(category) {
            buttons.forEach(b => b.classList.toggle("active", b.dataset.category === category));
            grids.forEach(g => g.classList.toggle("active", g.dataset.category === category));
          }

          // Handle clicks
          buttons.forEach(btn => {
            btn.addEventListener("click", () => {
              const category = btn.dataset.category;
              activateCategory(category);
            });
          });

          // Make sure Best Seller shows by default
          activateCategory("best-seller");
        });
    </script>

    @*  ABOUT SECTION SCRIPT *@
    <script>
        document.addEventListener("DOMContentLoaded", () => {
            const cards = document.querySelectorAll(".vmv-card");
            const overlay = document.createElement("div");
            overlay.classList.add("vmv-overlay");
            document.body.appendChild(overlay);

            cards.forEach(card => {
                card.addEventListener("click", () => {

                    if (card.classList.contains("active")) {
                        card.classList.remove("active");
                        overlay.classList.remove("active");
                    } else {

                        document.querySelectorAll(".vmv-card.active").forEach(c => c.classList.remove("active"));

                        card.classList.add("active");
                        overlay.classList.add("active");
                    }
                });
            });


            overlay.addEventListener("click", () => {
                document.querySelectorAll(".vmv-card.active").forEach(c => c.classList.remove("active"));
                overlay.classList.remove("active");
            });
        });
    </script>

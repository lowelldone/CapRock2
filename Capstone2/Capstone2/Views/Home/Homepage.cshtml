@{
    ViewData["Title"] = "Welcome to Grush Catering";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Grush Catering - Premium Catering Services</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.0/font/bootstrap-icons.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <style>
        :root {
            --primary-purple: #7c3aed;
            --primary-purple-dark: #6d28d9;
            --primary-purple-light: #a78bfa;
            --secondary-purple: #8b5cf6;
            --accent-purple: #c084fc;
            --purple-bg: #faf5ff;
            --purple-bg-light: #f3e8ff;
            --purple-border: #e9d5ff;
            --text-purple: #581c87;
            --text-purple-light: #7c3aed;
            --success-color: #10b981;
            --warning-color: #f59e0b;
            --danger-color: #ef4444;
            --info-color: #3b82f6;
        }

        body {
            font-family: 'Inter', sans-serif;
            color: #333;
            overflow-x: hidden;
        }


        /* Header & Navigation */
        .navbar {
            padding: .5rem 0;
            background: rgba(255, 255, 255, 0.95);
            box-shadow: 0 2px 15px rgba(0, 0, 0, 0.05);
            transition: all 0.3s ease;
        }

        .navbar-brand {
            font-weight: 700;
            color: var(--primary-purple);
            font-size: 1.8rem;
        }

            .navbar-brand:hover {
                color: var(--primary-purple);
            }

        .nav-link {
            font-weight: 500;
            color: #4a5568;
            margin: 0 0.5rem;
            transition: all 0.3s ease;
        }

            .nav-link:hover, .nav-link.active {
                color: var(--primary-purple);
            }

        .btn-primary {
            background: linear-gradient(135deg, var(--primary-purple) 0%, var(--secondary-purple) 100%);
            border: none;
            border-radius: 50px;
            padding: 0.6rem 1.8rem;
            font-weight: 600;
            transition: all 0.3s ease;
        }

            .btn-primary:hover {
                background: linear-gradient(135deg, var(--primary-purple-dark) 0%, var(--primary-purple) 100%);
                transform: translateY(-2px);
                box-shadow: 0 8px 25px rgba(124, 58, 237, 0.3);
            }

        /* Hero Section */
        .hero-section {
            background: linear-gradient(rgba(255, 255, 255, 0.9), rgba(250, 245, 255, 0.9)), url('https://images.unsplash.com/photo-1495195134817-aeb325a55b65?ixlib=rb-4.0.3&auto=format&fit=crop&w=1770&q=80');
            background-size: cover;
            background-position: center;
            padding: 7rem 0 5rem;
            position: relative;
        }

        .hero-content {
            position: relative;
            z-index: 2;
        }

        .hero-title {
            font-weight: 800;
            font-size: 3.5rem;
            color: var(--text-purple);
            margin-bottom: 1.5rem;
            line-height: 1.2;
        }

        .hero-subtitle {
            font-size: 1.25rem;
            color: #4a5568;
            margin-bottom: 2.5rem;
            line-height: 1.6;
        }

        .hero-badge {
            background: var(--purple-bg);
            color: var(--primary-purple);
            padding: 0.5rem 1.2rem;
            border-radius: 50px;
            font-weight: 600;
            display: inline-block;
            margin-bottom: 1.5rem;
            font-size: 0.9rem;
        }

        /* Stats Section */
        .stats-section {
            background: var(--purple-bg);
            padding: 4rem 0;
        }

        .stat-item {
            text-align: center;
        }

        .stat-number {
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--primary-purple);
            margin-bottom: 0.5rem;
        }

        .stat-label {
            color: #4a5568;
            font-weight: 500;
        }

        /*SERVICES SECTION*/
        .section-title.S {
            text-align: center;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            margin: 1rem auto;
            max-width: 900px;
            padding: 0.5rem 1rem;
        }

        .services-section {
            position: relative;
            padding: 6rem 0;
            background: url('images/SERVICES-BG.jpg') no-repeat center center/cover;
            z-index: 1;
        }

            .services-section::before {
                content: "";
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: rgba(255, 255, 255, 0.3);
                z-index: 0;
            }

            .services-section > * {
                position: relative;
                z-index: 2;
            }

        .section-title {
            text-align: center;
            margin-bottom: 3.5rem;
        }

            .section-title h2 {
                font-weight: 800;
                color: var(--text-purple);
                margin-bottom: 1rem;
                border-radius: 40px;
            }

            .section-title p {
                color: #6a17eb;
                max-width: 600px;
                margin: 0 auto;
                font-size: 1.2rem;
                display: inline-block;
                padding: 0.4rem 1rem;
                border-radius: 30px;
                text-align: center;
            }

        .service-card {
            background: #fff;
            border-radius: 1.2rem;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
            transition: all 0.3s ease;
            height: 100%;
        }

            .service-card:hover {
                transform: translateY(-10px);
                box-shadow: 0 15px 40px rgba(124, 58, 237, 0.15);
            }

        .service-img {
            height: 220px;
            overflow: hidden;
        }

            .service-img img {
                width: 100%;
                height: 100%;
                object-fit: cover;
                transition: transform 0.5s ease;
            }

        .service-card:hover .service-img img {
            transform: scale(1.05);
        }

        .service-content {
            padding: 1.8rem;
        }

        .service-title {
            font-weight: 700;
            color: var(--text-purple);
            margin-bottom: 1rem;
        }

        .service-desc {
            color: #718096;
            margin-bottom: 1.5rem;
        }

        .service-link {
            color: var(--primary-purple);
            font-weight: 600;
            text-decoration: none;
            display: inline-flex;
            align-items: center;
        }

            .service-link i {
                margin-left: 0.5rem;
                transition: transform 0.3s ease;
            }

            .service-link:hover {
                color: var(--primary-purple-dark);
            }

                .service-link:hover i {
                    transform: translateX(5px);
                }



        #service-images-modal {
            background-image: url('images/your-background.jpg');
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            background-blend-mode: overlay;
            background-color: rgba(0, 0, 0, 0.7);
            opacity: 0;
            visibility: hidden;
            transition: opacity 0.4s ease, visibility 0.4s ease;
            display: flex;
            justify-content: center;
            align-items: center;
            position: fixed;
            top: 0;
            left: 0;
            width: 100vw;
            height: 100vh;
            background: rgba(0, 0, 0, 0.7);
            z-index: 1050;
        }

            #service-images-modal.show {
                visibility: visible;
                opacity: 1;
            }

        #modal-image-container img {
            opacity: 0;
            transition: opacity 0.5s ease;
        }

            #modal-image-container img.show {
                opacity: 1;
            }
        /* REVIEW SECTION */
        .testimonial-section {
            background-image: url('images/TESTIMONIAL-BG.jpg');
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            padding: 6rem 0;
            color: #fff;
        }

        .Text-title p {
            font-size: 2rem;
            color: #6a17eb;
            font-weight: 800;
            text-align: center;
            margin-top: -1.5rem;
            margin-bottom: 1.8rem;
            display: inline-block;
            padding: 0.5rem 2rem;
            border-radius: 2rem;
            text-shadow: 0 0 12px #ffffff, 0 0 18px #ffffff, 0 4px 10px rgba(255, 255, 255, 0.7);
        }


        .Text-title {
            text-align: center;
        }

        .testimonial-card {
            background: #fff;
            border-radius: 1.2rem;
            padding: 2rem;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
            margin: 1rem;
            height: 100%;
        }

        .testimonial-text {
            font-size: 1.1rem;
            line-height: 1.5;
            color: #353232;
            margin-bottom: 1.8rem;
            position: relative;
        }

            .testimonial-text:before {
                content: """; font-size: 5rem; color: var(--purple-border);
                position: absolute;
                top: -1.5rem;
                left: -1rem;
                font-family: Georgia, serif;
                line-height: 1;
            }

        .section-title h2 {
            font-size: 2.2rem;
            font-weight: 800;
            margin-bottom: 0.4rem;
            text-align: center;
            color: #6a17eb;
            text-stroke: 1.2px #ffffff;
            text-shadow: 0 0 12px #fff, 0 0 20px #fff, 0 0 34px #fff, 0 0 46px #fff, 0 0 58px #fff;
            border-radius: 25px;
            display: inline-block;
            padding: 0.5rem 1.5rem;
            max-width: 80%;
            margin: 0 auto;
        }

        .testimonial-author {
            display: flex;
            align-items: center;
        }

        .testimonial-container {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 1rem;
        }

        .author-img {
            width: 60px;
            height: 60px;
            border-radius: 50%;
            overflow: hidden;
            margin-right: 1rem;
        }

            .author-img img {
                width: 100%;
                height: 100%;
                object-fit: cover;
            }

        .author-info h5 {
            font-weight: 700;
            margin-bottom: 0.2rem;
            color: var(--text-purple);
        }

        .author-info p {
            color: #718096;
            margin: 0;
        }
        /*MENU SECTION*/



        .menu-navigation {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 2rem;
            margin-top: 3rem;
            position: relative;
            z-index: 5;
        }

        .menu-section::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(255,255,255,0.5);
            z-index: 0;
        }

        .menu-section .container {
            position: relative;
            z-index: 2;
        }

        .menu-section {
            position: relative;
            background: url('../images/MENU-BG.jpg') no-repeat center center/cover;
            min-height: 8in;
            padding: 8rem 1rem 5rem;
            padding: 4rem 1rem;
            z-index: 1;
            padding: 2rem 0;
        }

        .menu-section {
            padding: 2rem 0;
        }

        .section-title h2 {
            font-size: 5rem;
            font-weight: 900;
            text-align: center;
            margin-bottom: 2rem;
        }

        .category-buttons {
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            gap: 0.75rem;
            margin-bottom: 2rem;
        }

        .category {
            padding: 0.6rem 1.2rem;
            border: none;
            border-radius: 8px;
            background: #f2f2f2;
            cursor: pointer;
            font-weight: 500;
            transition: background 0.3s;
        }

            .category.active {
                background-color: #5f3094;
                color: white;
            }

        .menu-grid {
            display: none;
            grid-template-columns: repeat(4, 1fr);
            gap: 2rem;
            padding: 2rem;
            max-width: 1200px;
            margin: 0 auto;
            justify-content: center;
            justify-items: center;
        }

            .menu-grid.active {
                display: grid;
            }

            /* Center grids with fewer items */
            .menu-grid[data-category="rice"] {
                grid-template-columns: repeat(3, 1fr);
                justify-content: center;
                max-width: 900px;
            }

            .menu-grid[data-category="beverage"] {
                grid-template-columns: repeat(4, 1fr);
                justify-content: center;
            }

            .menu-grid[data-category="dessert"] {
                grid-template-columns: repeat(4, 1fr);
                justify-content: center;
            }


        .menu-card {
            border-radius: 12px;
            box-shadow: 0 4px 6px rgba(0,0,0,0.1);
            overflow: hidden;
            text-align: center;
            padding: 0.75rem;
            display: none;
            width: 4in;
            height: auto;
            margin: auto;
            margin-bottom: 2rem;
        }


            .menu-card img {
                width: 100%;
                height: 100%;
                border-radius: 10px;
                object-fit: cover;
            }

            .menu-card h4 {
                margin: 0.5rem 0;
                font-size: 1.1rem;
            }

            .menu-card p {
                font-weight: bold;
                color: #333;
                margin-bottom: 0.75rem;
            }

        .menu-cards-row {
            display: flex;
            gap: 2rem;
            width: 100%;
            justify-content: center;
        }

        .menu-card.best-seller {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            gap: 2rem;
            width: 100%;
            max-width: none;
            margin: 0 auto;
            box-sizing: border-box;
        }

        .menu-card.best-seller1 {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            gap: 2rem;
            width: 100%;
            max-width: none;
            margin: 0 auto;
            box-sizing: border-box;
        }

        .menu-card.best-seller2 {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            gap: 2rem;
            width: 100%;
            max-width: none;
            margin: 0 auto;
            box-sizing: border-box;
        }

        .menu-card.best-seller3 {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            gap: 2rem;
            width: 100%;
            max-width: none;
            margin: 0 auto;
            box-sizing: border-box;
        }

        .best-seller-card {
            position: relative;
            aspect-ratio: 3 / 2;
            width: 100%;
            height: auto;
            border-radius: 15px;
            overflow: hidden;
            margin: 0;
        }

        .best-seller-card1 {
            position: relative;
            aspect-ratio: 3 / 2;
            width: 3in;
            height: 2in;
            border-radius: 15px;
            overflow: hidden;
            margin-top: 1rem;
            margin-bottom: 1rem;
        }

        .best-seller-card2 {
            position: relative;
            aspect-ratio: 3 / 2;
            width: 3in;
            height: 2in;
            border-radius: 15px;
            overflow: hidden;
            margin-top: 1rem;
            margin-bottom: 1rem;
        }

        .best-seller-card3 {
            position: relative;
            aspect-ratio: 3 / 2;
            width: 3in;
            height: 2in;
            border-radius: 15px;
            overflow: hidden;
            margin-top: 1rem;
            margin-bottom: 1rem;
        }


        .best-seller-card img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 15px;
            position: relative;
            z-index: 1;
            transition: transform 0.3s ease;
        }

        .best-seller-card1 img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 15px;
            position: relative;
            z-index: 1;
            transition: transform 0.3s ease;
        }

        .best-seller-card2 img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 15px;
            position: relative;
            z-index: 1;
            transition: transform 0.3s ease;
        }

        .best-seller-card3 img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            border-radius: 15px;
            position: relative;
            z-index: 1;
            transition: transform 0.3s ease;
        }

        .price-overlay {
            position: absolute;
            top: 0;
            right: -50%;
            width: 50%;
            height: 100%;
            background: linear-gradient(135deg, rgba(124, 58, 237, 0.9) 0%, rgba(139, 92, 246, 0.8) 100%);
            display: flex;
            justify-content: center;
            align-items: center;
            z-index: 2;
            transition: right 0.8s ease, opacity 0.5s ease;
            opacity: 0;
            pointer-events: none;
            border-radius: 15px 0 0 15px;
        }

            .price-overlay p {
                font-size: 1.5rem;
                font-weight: bold;
                color: white;
                opacity: 0;
                transition: opacity 0.4s ease 0.3s;
                text-align: center;
                text-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
            }

        .best-seller-card:hover img {
            transform: scale(1.05);
        }

        .best-seller-card1:hover img {
            transform: scale(1.05);
        }

        .best-seller-card2:hover img {
            transform: scale(1.05);
        }

        .best-seller-card3:hover img {
            transform: scale(1.05);
        }

        .best-seller-card:hover .price-overlay {
            right: 0;
            opacity: 1;
            pointer-events: auto;
        }

        .best-seller-card1:hover .price-overlay {
            right: 0;
            opacity: 1;
            pointer-events: auto;
        }

        .best-seller-card2:hover .price-overlay {
            right: 0;
            opacity: 1;
            pointer-events: auto;
        }

        .best-seller-card3:hover .price-overlay {
            right: 0;
            opacity: 1;
            pointer-events: auto;
        }

        .best-seller-card:hover .price-overlay p {
            opacity: 1;
        }

        .best-seller-card1:hover .price-overlay p {
            opacity: 1;
        }

        .best-seller-card2:hover .price-overlay p {
            opacity: 1;
        }

        .best-seller-card3:hover .price-overlay p {
            opacity: 1;
        }


        /*ABOUT SECTION*/

        .section-title.T h2 {
            font-size: 4rem;
            font-weight: 900;
            text-align: center;
            color: #6a17eb;
            text-shadow: 0 0 10px #fff, 0 0 20px #fff, 0 0 34px #fff, 0 0 44px #fff, 0 0 56px #fff;
            border-radius: 25px;
            display: inline-block;
            padding: 0.2rem 1rem;
            max-width: 80%;
            margin: 0.1rem auto 0;
            line-height: 1;
        }

        .section-title.P {
            text-align: center;
        }


        .section-title p {
            display: inline-block;
            white-space: nowrap;
            font-size: 1.2rem;
            font-weight: 500;
            font-family: 'Georgia', serif;
            color: #6a17eb;
            margin: 0 auto;
            margin-bottom: 0rem;
            line-height: 1.1;
        }

        .section-title.P h5 {
            font-size: 1.1rem;
            font-weight: 400;
            font-family: Arial, sans-serif;
            color: #2c1b41;
            margin-bottom: 0.3rem;
        }

        .about-section {
            background: url('../images/about-bj.jpg') no-repeat center center/cover;
            padding: 3rem 1rem;
            min-height: 400px;
            position: relative;
            z-index: 1;
        }

            .about-section::before {
                content: "";
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                /*    background: rgba(255, 255, 255, 0.3); */
                z-index: 0;
            }

        .vmv-card {
            width: 7in;
            height: 4in;
            background: rgba(167, 139, 250, 0.85);
            border-radius: 12px;
            overflow: hidden;
            position: relative;
            cursor: pointer;
            transition: transform 0.4s ease, box-shadow 0.4s ease;
        }

            .vmv-card img {
                width: 100%;
                height: 100%;
                object-fit: fill;
                display: block;
            }

            .vmv-card:hover {
                box-shadow: 0 8px 14px #8b5cf6;
            }

            .vmv-card.active {
                position: fixed;
                top: 50%;
                left: 50%;
                width: 7.5in;
                height: 5.5in;
                transform: translate(-50%, -50%) scale(1.05);
                z-index: 999;
                box-shadow: 0 10px 30px rgba(0,0,0,0.6);
                background: rgba(167, 139, 250, 0.95);
            }


        .about-images {
            position: relative;
            z-index: 2;
            display: flex;
            justify-content: center;
            gap: 1.5rem;
            flex-wrap: nowrap;
        }


        .about-section::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            /*   background: rgba(255, 255, 255, 0.3); */
            z-index: 0;
        }

        .cta-section {
            background-image: linear-gradient( rgba(241, 7, 163, 0.2) 0%), url('images/contact-BG.jpg');
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            min-height: 360px;
            padding: 2rem 1rem;
            text-align: center;
        }

        .cta-title {
            font-weight: 800;
            margin-bottom: 1rem;
            font-size: 3.5rem;
            color: #fff;
        }

        .cta-text {
            font-size: 1.2rem;
            max-width: 700px;
            margin: 0 auto 2.5rem;
            opacity: 0.9;
            color: #fff;
        }

        /* FOOTER */
        #contact-section {
            background-color: #2f2f2f;
            color: white;
            padding: 2rem 0;
        }

        .footer {
            background: #1a202c;
            color: white;
            padding: 2rem 0 1rem;
        }

        .footer-title {
            font-weight: 700;
            margin-bottom: 1.5rem;
            color: white;
        }

        .footer-links {
            list-style: none;
            padding: 0;
        }

            .footer-links li {
                margin-bottom: 0.8rem;
            }

            .footer-links a {
                color: #a0aec0;
                text-decoration: none;
                transition: all 0.3s ease;
            }

                .footer-links a:hover {
                    color: white;
                }

        .footer-contact {
            color: white;
            margin-bottom: 0.8rem;
            display: flex;
            align-items: center;
        }

            .footer-contact i {
                margin-right: 0.8rem;
                color: var(--primary-purple-light);
            }

        .social-links {
            display: flex;
            margin-top: 1.5rem;
        }

            .social-links a {
                width: 40px;
                height: 40px;
                border-radius: 50%;
                background: rgba(255, 255, 255, 0.1);
                display: flex;
                align-items: center;
                justify-content: center;
                margin-right: 1rem;
                transition: all 0.3s ease;
            }

                .social-links a:hover {
                    background: var(--primary-purple);
                    transform: translateY(-3px);
                }

